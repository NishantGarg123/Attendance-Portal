<% layout("/layouts/boilerplate.ejs") %>


    <body>
        <h3 class="mt-3">All students of the <%= year %> year <%= branch> </h3>

        <div class="mt-3 mb-3">
            <table class="table table-striped">
                <tr>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Edit student details</th>
                    <th>Delete Student</th>
                </tr>
            <% for(student of result){ %>
    
                <tr>
                    <td><%= student.username %></td>
                    <td><%= student.email %></td>
                    <td><a href="/attendance/head/studentsInfo/<%= student._id %>/edit"><button class="btn btn-dark">Edit</button></a></td>
                    <td><a href="/attendance/head/studentsInfo/<%= student._id %>/delete" ><button class="btn btn-dark">delete</button></a></td>
                </tr>
                <% } %>
            </table>
        </div>
        
    </body>