<% layout("/layouts/boilerplate.ejs") %>
<body>
    <h2 class="head-heading">Welcome our Admin page</h2>
    <div class="row">
        <div class="col-8">
            <form method="post" action="/attendance/head" class="needs-validation" novalidate>
            
                <label for="email">email:</label>
                <input type="email" name="email" placeholder="enter email" class="form-control mb-3" required>
                <div class="invalid-feedback">please enter the valid email</div>
        
                <label for="password">Password:</label>
                <input type="password" name="password" placeholder="enter password" class="form-control mb-4" required>
                <div class="invalid-feedback">please enter the password</div>

                <button class="btn btn-dark">submit</button>
                </form>
        </div>
    </div>
    
</body>